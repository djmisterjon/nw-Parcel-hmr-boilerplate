{
   "author": "@SpectorJS",
   "background": {
      "persistent": true,
      "scripts": [ "background.js" ]
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "spector.bundle.func.js", "contentScript.js" ],
      "matches": [ "file://*/*", "http://*/*", "https://*/*" ],
      "run_at": "document_start"
   } ],
   "description": "Explore and Troubleshoot your WebGL scenes easily.",
   "homepage_url": "http://spector.babylonjs.com/",
   "icons": {
      "128": "spectorjs-128.png",
      "48": "spectorjs-48.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "20",
   "name": "Spector.js",
   "page_action": {
      "default_icon": {
         "19": "spectorjs-19.png",
         "38": "spectorjs-38.png"
      },
      "default_title": "Debug with Spector"
   },
   "permissions": [ "tabs", "http://*/*", "https://*/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.9.16",
   "web_accessible_resources": [ "spector.bundle.js", "popup.js", "result.js" ]
}
